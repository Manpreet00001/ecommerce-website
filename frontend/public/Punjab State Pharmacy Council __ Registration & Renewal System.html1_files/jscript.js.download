                function upload_file(e){                   
                var form_data = new FormData();              
                form_data.append('_file', document.getElementById(e).files[0]);
                form_data.append('form_data',$('#form_data_'+e).val);
                $.ajax({
                                    cache: false,
                                    type: "POST",
                                    url:base_url+'UploadDocumetns',
                                    data: form_data,
                                    dataType: "text",
                                    contentType: false,
                                    processData: false,
                                    success:function(data){
                                    const resp = JSON.parse(data);
                                    var result=window.atob(resp.result);
                                    if(resp.status===true){
                                        $('#'+e).attr('type', 'text');
                                    }else{
                                        //$('#upload_response').html(result);
                                        alert(result);
                                    }
                                    }
                                }); 
                }
                
                function mysql_date(d){
                    //This function should be used to change date format from dd/mm/yyyy to yyyy-mm-dd//
                    var db_date=d.split('/');
                    var db_date=(db_date[2]+'-'+db_date[1]+'-'+db_date[0]);
                    return db_date;
                }
